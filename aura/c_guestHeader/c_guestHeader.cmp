<aura:component extends="forceCommunity:navigationMenuBase">
    <aura:attribute name="navigationLinkSetId" type="String" default="Guest_Navigation" />
    
    <div class="mainHeader">
        <div class="pageSize">
            <div class="headerFlex">
                <div class="headerLogo">
                    <a href="javascript:void(0);">
                        <img class="logoWidth" src="https://afdhealth.file.force.com/sfc/dist/version/download/?oid=00DHs000001QFbz&amp;ids=068Hs00000YfdUC&amp;d=%2Fa%2FHs0000012wEv%2FznB8vuTmKGILFKjlAsR30Im7Tu8q59bS9VI07pd6z1M&amp;asPdf=false" alt="AFD Logo" />
                    </a>
                </div>
                <div class="headerMenu">
                   <label for="show-menu" class="show-menu">Show Menu</label>
                    <input type="checkbox" id="show-menu" role="button"></input>
                    <ul id="menu" onclick="{!c.navigateToMenu}">
                        <aura:iteration items="{!v.menuItems}" var="item">
                            <li class="{!item.active ? 'slds-is-active' : ''}">
                                <aura:renderIf isTrue="{!if(item.label == 'Home', true, false)}">
                                    <a href="javascript:void(0);" data-menu-item-id="{!item.id}">
                                        {!item.label} 
                                    </a>
                                </aura:renderIf>
                                
                                <aura:renderIf isTrue="{!if(item.label == 'Services', true, false)}">
                                    <a href="javascript:void(0);" data-menu-item-id="{!item.id}">
                                        {!item.label} 
                                        <img class="downChild" src="https://afdhealth.file.force.com/sfc/dist/version/renditionDownload?rendition=ORIGINAL_Png&amp;versionId=068Hs00000YiFJt&amp;operationContext=DELIVERY&amp;contentId=05THs00001sobos&amp;page=0&amp;d=/a/Hs0000012zIR/dpVH8ILj35iLilTzO9Va6VQ1mFxCpeHufuSahaAD1NA&amp;oid=00DHs000001QFbz&amp;dpt=null&amp;viewId="/>
                                    </a>
                                    <ul class="sub-menu">
                                        <aura:iteration items="{!item.subMenu}" var="subItem">
                                            <li data-menu-item-id="{!subItem.id}" onclick="{!c.navigateToMenu}">
                                                <a data-menu-item-id="{!subItem.id}" href="javascript:void(0);" onclick="{!c.navigateToMenu}">
                                                    <aura:renderIf isTrue="{!if(subItem.label == 'Prescriptions', true, false)}">
                                                                <img src="{!$Resource.TMD_Assets + '/images/prescriptions-menu-icon.png'}" data-menu-item-id="{!subItem.id}" onclick="{!c.navigateToMenu}"/>
                                                            </aura:renderIf>
                                                            <aura:renderIf isTrue="{!if(subItem.label == 'Urgent Care', true, false)}">
                                                                <img src="{!$Resource.TMD_Assets + '/images/urgent-menu-icon.png'}" data-menu-item-id="{!subItem.id}" onclick="{!c.navigateToMenu}"/>
                                                            </aura:renderIf>
                                                            <aura:renderIf isTrue="{!if(subItem.label == 'Mental Health', true, false)}">
                                                                <img src="{!$Resource.TMD_Assets + '/images/mental-menu-icon.png'}" data-menu-item-id="{!subItem.id}" onclick="{!c.navigateToMenu}"/>
                                                            </aura:renderIf>
                                                            <aura:renderIf isTrue="{!if(subItem.label == 'Weight Loss', true, false)}">
                                                                <img src="{!$Resource.TMD_Assets + '/images/weight-menu-icon.png'}" data-menu-item-id="{!subItem.id}" onclick="{!c.navigateToMenu}"/>
                                                            </aura:renderIf>
                                                            <aura:renderIf isTrue="{!if(subItem.label == 'Interactive Medicine', true, false)}">
                                                                <img src="{!$Resource.TMD_Assets + '/images/integrative-menu-icon.png'}" data-menu-item-id="{!subItem.id}" onclick="{!c.navigateToMenu}"/>
                                                            </aura:renderIf>
                                                   
													<div class="menuInner" data-menu-item-id="{!subItem.id}" onclick="{!c.navigateToMenu}">
                                                    	{!subItem.label}
                                                        <span data-menu-item-id="{!subItem.id}" onclick="{!c.navigateToMenu}">
                                                            <aura:renderIf isTrue="{!if(subItem.label == 'Prescriptions', true, false)}">
                                                                Your Convenient Healthcare Solution
                                                            </aura:renderIf>
                                                            <aura:renderIf isTrue="{!if(subItem.label == 'Urgent Care', true, false)}">
                                                                Healthcare When You Need It
                                                            </aura:renderIf>
                                                            <aura:renderIf isTrue="{!if(subItem.label == 'Mental Health', true, false)}">
                                                                Nurturing Your Inner Health
                                                            </aura:renderIf>
                                                            <aura:renderIf isTrue="{!if(subItem.label == 'Weight Loss', true, false)}">
                                                                Weight Loss Made Simple
                                                            </aura:renderIf>
                                                            <aura:renderIf isTrue="{!if(subItem.label == 'Interactive Medicine', true, false)}">
                                                                Enhancing Wellness Naturally
                                                            </aura:renderIf>
                                                        </span>
                                                    </div>
                                                </a>
                                            </li>
                                        </aura:iteration>
                                    </ul>
                                </aura:renderIf>
                                <aura:renderIf isTrue="{!if(item.label == 'AFD Clinic', true, false)}">
                                    <a href="javascript:void(0);" data-menu-item-id="{!item.id}" onclick="{!c.navigateToMenu}">
                                        {!item.label} 
                                        <img class="downChild" src="https://afdhealth.file.force.com/sfc/dist/version/renditionDownload?rendition=ORIGINAL_Png&amp;versionId=068Hs00000YiFJt&amp;operationContext=DELIVERY&amp;contentId=05THs00001sobos&amp;page=0&amp;d=/a/Hs0000012zIR/dpVH8ILj35iLilTzO9Va6VQ1mFxCpeHufuSahaAD1NA&amp;oid=00DHs000001QFbz&amp;dpt=null&amp;viewId="/>
                                    </a>
                                    <ul class="sub-menu">
                                        <aura:iteration items="{!item.subMenu}" var="subItem">
                                            <li data-menu-item-id="{!subItem.id}" onclick="{!c.navigateToMenu}">
                                                <a href="javascript:void(0);" data-menu-item-id="{!subItem.id}" onclick="{!c.navigateToMenu}">
                                                    <aura:renderIf isTrue="{!if(subItem.label == 'About Us', true, false)}">
                                                                <img src="{!$Resource.TMD_Assets + '/images/about-menu-icon.png'}" data-menu-item-id="{!subItem.id}" onclick="{!c.navigateToMenu}"/>
                                                            </aura:renderIf>
                                                            <aura:renderIf isTrue="{!if(subItem.label == 'FAQ', true, false)}">
                                                                <img src="{!$Resource.TMD_Assets + '/images/faq-menu-icon.png'}" data-menu-item-id="{!subItem.id}" onclick="{!c.navigateToMenu}"/>
                                                            </aura:renderIf>
                                                            
                                                   
													<div class="menuInner" data-menu-item-id="{!subItem.id}" onclick="{!c.navigateToMenu}">
                                                    	{!subItem.label}
                                                        <span>
                                                            <aura:renderIf isTrue="{!if(subItem.label == 'About Us', true, false)}">
                                                                Get to Know Us
                                                            </aura:renderIf>
                                                            <aura:renderIf isTrue="{!if(subItem.label == 'FAQ', true, false)}">
                                                               Common Questions
                                                            </aura:renderIf>
                                                        </span>
                                                    </div>
                                                </a>
                                            </li>
                                        </aura:iteration>
                                    </ul>
                                </aura:renderIf>
                            </li>
                        </aura:iteration>
                        <li class="loginBtn">
                            <a href="https://afdhealth--partial.sandbox.my.site.com/afd/s/login/" target="_blank">
                                <img class="logoWidth" src="https://afdhealth.file.force.com/sfc/dist/version/renditionDownload?rendition=ORIGINAL_Png&amp;versionId=068Hs00000YiFJy&amp;operationContext=DELIVERY&amp;contentId=05THs00001sobrW&amp;page=0&amp;d=/a/Hs0000012zIC/9DQGt94_JkZ1Wn9JCOtDWWTmH.HKcUXXd2Jr6o_xwiM&amp;oid=00DHs000001QFbz&amp;dpt=null&amp;viewId=" alt="Login Logo" />
                                Login
                            </a>
                        </li>
                    </ul>
                </div>
            </div>
        </div>
    </div>
</aura:component>